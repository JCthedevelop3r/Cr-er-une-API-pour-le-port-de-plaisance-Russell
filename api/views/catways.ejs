<div class="catways-container">
  <h1>Catways</h1>
  <!-- Exemple temporaire -->
  <table>
    <thead>
      <tr>
        <th class="th-catway-number">Numéro du catway</th>
        <th class="th-availability">Disponibilité</th>
      </tr>
    </thead>
    <tbody>
      <% catways.forEach(catway => { %>
      <tr>
        <td class="td-catway-number">
          <a href="/catways/<%= catway._id %>"><%= catway.catwayNumber %></a>
        </td>
        <td class="td-availability">oui/non</td>
      </tr>
      <% }); %>
    </tbody>
  </table>
</div>
